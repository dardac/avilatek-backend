    "express-rate-limit": "7.5.1",
    "jsonwebtoken": "9.0.2",
    "p-retry": "6.2.1",
    "swagger-ui-express": "5.0.1",
    "ws": "8.18.2"
  },
  "devDependencies": {
    "@types/express": "5.0.3",
    "@types/express-rate-limit": "6.0.2",
    "@types/jsonwebtoken": "9.0.10",
